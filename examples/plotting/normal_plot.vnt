[infer (resample 10)]
[assume x (normal 0 1)]
[assume y (normal x 1)]
[infer
 (let ((d (empty)))
   (do (repeat 100
        (do (mh default all 1)
            (into d (run (collect x y (abs (- y x)))))))
       (plotf_to_file
        (list "normal1" "normal2" "normal3" "normal4")
        (list "lctl" "pc0r" "h0" "h1")
        d)))]
